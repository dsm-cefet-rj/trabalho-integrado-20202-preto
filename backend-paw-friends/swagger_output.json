  "definitions": {
    "User": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "example": "Jhon Doe"
        },
        "age": {
          "type": "number",
          "example": 29
        },
        "parents": {
          "type": "object",
          "properties": {
            "father": {
              "type": "string",
              "example": "Simon Doe"
            },
            "mother": {
              "type": "string",
              "example": "Marie Doe"
            }
          }
        },
        "diplomas": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "school": {
                "type": "string",
                "example": "XYZ University"
              },
              "year": {
                "type": "number",
                "example": 2020
              },
              "completed": {
                "type": "boolean",
                "example": true
              },
              "internship": {
                "type": "object",
                "properties": {
                  "hours": {
                    "type": "number",
                    "example": 290
                  },
                  "location": {
                    "type": "string",
                    "example": "XYZ Company"
                  }
                }
              }
            }
          }
        }
      }
    },
    "AddUser": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "example": "Jhon Doe"
        },
        "age": {
          "type": "number",
          "example": 29
        },
        "about": {
          "type": "string",
          "example": ""
        }
      },
      "required": [
        "name",
        "age"
      ]
    },
    "Anuncio": {
      "type": "object",
      "properties": {
        "_id": {
          "type": "object",
          "properties": {
            "oid": {
              "type": "string",
              "example": "6078909e4cae7a39ec3be355"
            }
          },
          "required": [
            "oid"
          ]
        },
        "usuario": {
          "type": "string",
          "example": "filipeshanom"
        },
        "cardDescricao": {
          "type": "string",
          "example": "Gatinho para adoção muito simpático."
        },
        "temperamento": {
          "type": "string",
          "example": "Tranquilo"
        },
        "img": {
          "type": "string",
          "example": "https://i2-prod.mirror.co.uk/incoming/article21142078.ece/ALTERNATES/s615/0_Stray-Cat.jpg"
        },
        "descricao": {
          "type": "string",
          "example": "Quero dar esse gato fofo para adoção pois não tenho recursos financeiros para cuidar dele. Ele é bem agitado, gosta de bater nas pessoas e provavelmente vai acabar com a sua casa. Mas ele é lindo. Olha pra essa cara fofa."
        },
        "idade": {
          "type": "number",
          "example": 2
        },
        "sexo": {
          "type": "string",
          "example": "Macho"
        },
        "raça": {
          "type": "string",
          "example": "Desconhecida"
        },
        "tipo": {
          "type": "string",
          "example": "Gato"
        },
        "nome": {
          "type": "string",
          "example": "Salvador"
        },
        "dataAnuncio": {
          "type": "object",
          "properties": {
            "date": {
              "type": "string",
              "example": "2021-04-29T22:07:45.000Z"
            }
          },
          "required": [
            "date"
          ]
        },
        "__v": {
          "type": "number",
          "example": 0
        }
      },
      "required": [
        "_id",
        "usuario",
        "cardDescricao",
        "temperamento",
        "img",
        "descricao",
        "idade",
        "sexo",
        "raça",
        "tipo",
        "nome",
        "dataAnuncio",
        "__v"
      ]
    },
    "ArrayAnuncios": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "object",
            "properties": {
              "oid": {
                "type": "string",
                "example": "607892064cae7a39ec3be357"
              }
            },
            "required": [
              "oid"
            ]
          },
          "usuario": {
            "type": "string",
            "example": "gabs452"
          },
          "cardDescricao": {
            "type": "string",
            "example": "Golden filhote em niterói para adoção"
          },
          "temperamento": {
            "type": "string",
            "example": "Bagunceira"
          },
          "img": {
            "type": "string",
            "example": "https://i.pinimg.com/originals/0c/e8/cd/0ce8cddcbbb19d11199fa8ae2d81927f.jpg"
          },
          "descricao": {
            "type": "string",
            "example": " A Laurita é linda demais, adora crianças. Uma companheira para todas as horas!"
          },
          "idade": {
            "type": "number",
            "example": 1
          },
          "sexo": {
            "type": "string",
            "example": "Fêmea"
          },
          "raça": {
            "type": "string",
            "example": "Golden"
          },
          "tipo": {
            "type": "string",
            "example": "Cachorro"
          },
          "nome": {
            "type": "string",
            "example": "Laurita"
          },
          "dataAnuncio": {
            "type": "object",
            "properties": {
              "date": {
                "type": "string",
                "example": "2021-04-15T19:20:37.000Z"
              }
            },
            "required": [
              "date"
            ]
          },
          "__v": {
            "type": "number",
            "example": 0
          }
        },
        "required": [
          "_id",
          "usuario",
          "cardDescricao",
          "temperamento",
          "img",
          "descricao",
          "idade",
          "sexo",
          "raça",
          "tipo",
          "nome",
          "dataAnuncio",
          "__v"
        ]
      }
    }
  }
}